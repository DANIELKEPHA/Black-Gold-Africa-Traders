{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "rimraf dist && node --max-old-space-size=2048 node_modules/.bin/tsc",
    "start": "npm run build && node dist/index.js",
    "dev": "npm run build && concurrently \"npx tsc -w\" \"nodemon --exec ts-node src/index.ts\"",
    "seed": "ts-node src/prisma/seed.ts",
    "prisma:generate": "npx prisma generate --schema=src/prisma/schema.prisma",
    "prisma:push": "npx prisma db push --schema=src/prisma/schema.prisma",
    "postprisma:generate": "shx cp -r node_modules/.prisma/client/index.d.ts ../client/src/types/prismaTypes.d.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
