    "@aws-sdk/lib-storage": "^3.787.0",
    "@prisma/client": "^6.7.0",
    "@terraformer/wkt": "^2.2.1",
    "@types/csv-parse": "^1.1.12",
    "aws-jwt-verify": "^5.0.0",
    "aws-sdk": "^2.1692.0",
    "axios": "^1.10.0",
    "body-parser": "^2.2.0",
    "cors": "^2.8.5",
    "csv-parse": "^5.6.0",
    "csv-stringify": "^6.5.2",
    "csv-writer": "^1.6.0",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.1",
    "express-validator": "^7.2.1",
    "fast-csv": "^5.0.2",
    "helmet": "^8.1.0",
    "json2csv": "^6.0.0-alpha.2",
    "jsonwebtoken": "^9.0.2",
    "jwk-to-pem": "^2.0.7",
    "jwks-rsa": "^3.2.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.2",
    "pdfkit": "^0.17.1",
    "sanitize-html": "^2.17.0",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/aws-sdk": "^0.0.42",
